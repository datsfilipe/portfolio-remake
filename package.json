{
	"name": "datsfilipe.dev",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "bun start & bun generate-assets:watch",
		"start": "vite --port 3000",
		"build": "bun run pre-build && bun run generate-assets && tsc -b && vite build",
		"preview": "vite preview",
		"lint": "biome lint --error-on-warnings --write ./src",
		"format": "biome format --write ./src",
		"pre-build": "bun run src/shared/lib/pre-build.ts",
		"generate-assets": "bun run src/shared/lib/generatePosts.ts && bun run src/shared/lib/generateNotes.ts",
		"generate-assets:watch": "bun run src/shared/lib/generatePosts.ts watch && bun run src/shared/lib/generateNotes.ts watch"
	},
	"dependencies": {
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-icons": "^5.2.1",
		"react-router-dom": "^6.25.1",
		"gray-matter": "^4.0.3"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.8.3",
		"@shikijs/rehype": "^1.11.1",
		"@tailwindcss/typography": "^0.5.13",
		"@types/react": "^18.3.3",
		"@types/react-dom": "^18.3.0",
		"@vitejs/plugin-react-swc": "^3.5.0",
		"autoprefixer": "^10.4.19",
		"bun-types": "^1.1.20",
		"postcss": "^8.4.39",
		"rehype-autolink-headings": "^7.1.0",
		"rehype-katex": "^7.0.0",
		"rehype-slug": "^6.0.0",
		"rehype-stringify": "^10.0.0",
		"remark-math": "^6.0.0",
		"remark-parse": "^11.0.0",
		"remark-rehype": "^11.1.0",
		"tailwind-variants": "^0.2.1",
		"tailwindcss": "^3.4.6",
		"typescript": "^5.2.2",
		"unified": "^11.0.5",
		"vite": "^5.3.4",
		"vite-plugin-rss": "^0.0.6",
		"zod": "^3.23.8"
	}
}