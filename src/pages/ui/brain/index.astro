---
import BrainNote from '../../layouts/BrainNote.astro';

import { CollectionEntry, getCollection } from 'astro:content';

type Props = CollectionEntry<'brain'>;

const indexNote = (await getCollection('brain')).find(note => note.slug === 'readme') as Props;
const { Content } = await indexNote.render();
---
<BrainNote {...indexNote.data}>
  <Content/>
</BrainNote>